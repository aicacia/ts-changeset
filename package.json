{
    "name": "@stembord/changeset",
    "version": "0.1.3",
    "description": "ecto like changesets for typescript",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/stembord/ts-changeset.git"
    },
    "main": "./src/index.js",
    "types": "./src/index.d.ts",
    "module": "./lib/index.ts",
