    "readmeFilename": "README.md",
    "devDependencies": {
        "@types/tape": "^4.2.32",
        "parcel-bundler": "^1.9.7",
        "tap-spec": "^5.0.0",
        "tape": "^4.9.1",
        "ts-node": "^7.0.0",
        "typescript": "^3.0.1"
    },
    "dependencies": {}
}
